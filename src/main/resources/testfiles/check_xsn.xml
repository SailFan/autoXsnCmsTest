<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="testxsn">
	<test name="login">
		<groups>
			<run>
				<include name="check"></include>
			</run>
		</groups>
		<classes>
			<class name="org.xsn.autotest.testcase.TestLoginPage"></class>
		</classes>
	</test>
	<test name="modifypassword">
		<groups>
			<run>
				<include name="check"></include>
			</run>
		</groups>
		<classes>
			<class name="org.xsn.autotest.testcase.TestModify"></class>
		</classes>
	</test>
	<test name="home">
		<groups>
			<run>
				<include name="check"></include>
			</run>
		</groups>
		<classes>
			<class name="org.xsn.autotest.testcase.TestHomePage"></class>
		</classes>
	</test>
	
	
	<!-- 这样执行肯定要面临一个问题就是浏览器初始化的问题，问浏览器初始化什么时候执行 -->
	<!-- 通过分组来运行来运行，分好多好多的组 -->
</suite>